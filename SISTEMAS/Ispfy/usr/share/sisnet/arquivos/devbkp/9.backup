    ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source=":\
    log info \"Backup iniciado\"\r\
    \n:global backupfile ([/system identity get name] . \"-\" . [/system clock\
    \_get time])\r\
    \n/system backup save name=\$backupfile\r\
    \n:log info \"Aguarde. Arquivo sendo compilado\"\r\
    \n:delay 10s\r\
    \n:log info \"Criando e enviando email\"\r\
    \n/tool e-mail send to=\"backupcentralisp@gmail.com\" subject=([/system id\
    entity get name] . \\\r\
    \n\" Backup\") from=backupcentralisp@gmail.com file=\$backupfile server=sm\
    tp.gmail.com\r\
    \n:log info \"Excluindo arquivo local de backup\"\r\
    \n:delay 10s\r\
    \n:foreach i in=[/file find] do={:if ([:typeof [:find [/file get \$i name]\
    \_\".backup\"]]!=\"nil\") do={/file remove \$i}}\r\
    \n:log info \"Backup Finalizado com Sucesso!\""
